{% extends 'base.html.twig' %}

{% block title %}Adsense{% endblock %}

{% block body %}
    <h1>Adsense</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adsense.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ adsense.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ adsense.description }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ adsense.body }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ adsense.slug }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ adsense.createdAt ? adsense.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ adsense.updatedAt ? adsense.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adsense_index') }}">back to list</a>

    <a href="{{ path('adsense_edit', {'id': adsense.id}) }}">edit</a>

    {{ include('adsense/_delete_form.html.twig') }}
{% endblock %}
